  "version": "1.0.0",
  "author": "Kyle Pollich <kpollich1@gmail.com>",
  "dependencies": {
    "bulma": "^0.7.5",
    "gatsby": "2.13.57",
    "gatsby-image": "^2.2.8",
    "gatsby-link": "2.2.4",
    "gatsby-plugin-offline": "^2.2.5",
    "gatsby-plugin-react-helmet": "3.1.3",
    "gatsby-plugin-sass": "2.1.5",
    "gatsby-plugin-typescript": "^2.1.2",
    "gatsby-remark-prismjs": "3.3.5",
    "gatsby-source-contentful": "2.1.19",
    "gatsby-source-filesystem": "2.1.9",
    "gatsby-transformer-json": "2.2.2",
    "gatsby-transformer-remark": "2.6.12",
    "node-sass": "^4.12.0",
    "prismjs": "^1.17.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --no-semi --single-quote --write 'src/**/*.{ts,tsx}'",
    "lint": "eslint src/**/*.js",
    "precommit": "lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "yarn format"
    ]
  },
  "devDependencies": {
    "@types/prop-types": "^15.7.1",
    "@types/react": "^16.9.1",
    "@types/react-helmet": "^5.0.9",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.3",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2"
  },
  "apollo": {
    "schemas": {
      "gatsby": {
        "endpoint": "http://localhost:8000/___graphql"
      }
    },
    "queries": {
      "schema": "gatsby",
      "includes": [
        "./src/**/*.js"
      ],
      "excludes": [
        "node_modules/**"
      ]
    }
  }
}
