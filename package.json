{
  "name": "kpollich.github.io",
  "description": "Kyle Pollich Personal Site/Blog",
  "version": "1.0.0",
  "author": "Kyle Pollich <kpollich1@gmail.com>",
  "dependencies": {
    "bulma": "^0.7.2",
    "gatsby": "2.0.85",
    "gatsby-image": "^2.0.25",
    "gatsby-link": "2.0.7",
    "gatsby-plugin-offline": "^2.0.21",
    "gatsby-plugin-react-helmet": "3.0.5",
    "gatsby-plugin-sass": "2.0.7",
    "gatsby-plugin-typescript": "^2.0.3",
    "gatsby-remark-prismjs": "3.2.0",
    "gatsby-source-contentful": "2.0.22",
    "gatsby-source-filesystem": "2.0.12",
    "gatsby-transformer-json": "2.1.6",
    "gatsby-transformer-remark": "2.1.19",
    "node-sass": "^4.11.0",
    "prismjs": "^1.15.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --no-semi --single-quote --write 'src/**/*.{ts,tsx}'",
    "lint": "eslint src/**/*.js",
    "precommit": "lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "yarn format"
    ]
  },
  "devDependencies": {
    "@types/prop-types": "^15.5.8",
    "@types/react": "^16.7.18",
    "@types/react-helmet": "^5.0.8",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3"
  },
  "apollo": {
    "schemas": {
      "gatsby": {
        "endpoint": "http://localhost:8000/___graphql"
      }
    },
    "queries": {
      "schema": "gatsby",
      "includes": [
        "./src/**/*.js"
      ],
      "excludes": [
        "node_modules/**"
      ]
    }
  }
}
